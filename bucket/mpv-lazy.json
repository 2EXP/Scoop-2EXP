{
  "version": "20210224",
  "description": "mpv lazy profile",
  "homepage": "https://bbs.vcb-s.com/thread-5843-1-1.html",
  "depends": "mpv-git",
  "notes": [
    "Original mpv profile will be backuped in mpv-lazy persist directory,",
    "and will be restored after mpv-lazy uninstallation."
  ],
  "url": "https://static.cowtransfer.com/cowtransfer-file-49ba3f1c-0d9e-4436-91f7-164c15d48f31%252Fmpv-lazy-20210224.7z?t-s=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJndWlkK3RpbWVzdGFtcCI6Imlsb3ZlY293dHJhbnNmZXIyMDIxXzE2MTUyNzYzNTM4MjIifQ.h0xNdOm-SdtGp4rm4hHwdG1ktq57neeFeumeuWQ3rVE&t-c=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJndWlkK3RpbWVzdGFtcCI6IjIwMjF8M3w5fDE1In0.TWgyN_lNpA03EpvZUMk5nfmJ1q0P-QczIjRgQW33VC0&attname=mpv-lazy-20210224.7z",
  "hash": "895E65B332B2FEB0AAFBCA0B3DAEE4071EC9C3CDC406D4BD267B1952A0F06BFA",
  "extract_dir": "mpv-lazy\\portable_config",
  "installer": {
    "script": [
      "{",
      "  $mpv = (Split-Path $persist_dir) + '\\mpv-git'",
      "  Rename-Item $mpv mpv-lazy",
      "  New-Item -ItemType Junction -Path $mpv -Value $dir",
      "}"
    ]
  },
  "uninstaller": {
    "script": [
      "{",
      "  $persist = Split-Path $persist_dir",
      "  Remove-Item ($persist + '\\mpv-git')",
      "  Rename-Item ($persist + '\\mpv-lazy') mpv-git",
      "}"
    ]
  }
}
