{
  "version": "20210224",
  "description": "mpv lazy profile",
  "homepage": "https://bbs.vcb-s.com/thread-5843-1-1.html",
  "depends": "mpv-git",
  "notes": [
    "Original mpv profile will be backuped in mpv-lazy persist directory,",
    "and will be restored after mpv-lazy uninstallation."
  ],
  "url": "https://static.cowtransfer.com/cowtransfer-file-49ba3f1c-0d9e-4436-91f7-164c15d48f31%252Fmpv-lazy-20210224.7z?t-s=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJndWlkK3RpbWVzdGFtcCI6Imlsb3ZlY293dHJhbnNmZXIyMDIxXzE2MTUyODQ0MjY2MDQifQ.hSkmJ7H5G1fVzkxoN3IXteWpCnB6v9V4tEZ97kRSfMk&t-c=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJndWlkK3RpbWVzdGFtcCI6IjIwMjF8M3w5fDE4In0.zXi57Sns3JnUcBX5aq623O4EDcVvkKu4YPSUVVEVtTc&attname=mpv-lazy-20210224.7z",
  "hash": "895E65B332B2FEB0AAFBCA0B3DAEE4071EC9C3CDC406D4BD267B1952A0F06BFA",
  "extract_dir": "mpv-lazy\\portable_config",
  "installer": {
    "script": [
      "{",
      "  $mpv = (Split-Path $persist_dir) + '\\mpv-git\\portable_config'",
      "  Rename-Item $mpv config_backups",
      "  New-Item -ItemType Junction -Path $mpv -Value $dir",
      "}"
    ]
  },
  "uninstaller": {
    "script": [
      "{",
      "  $mpv = (Split-Path $persist_dir) + '\\mpv-git'",
      "  Remove-Item ($mpv + '\\portable_config')",
      "  Rename-Item ($mpv + '\\config_backups') portable_config",
      "}"
    ]
  }
}
